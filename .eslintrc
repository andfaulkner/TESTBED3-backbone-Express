{
  "parser": "babel-eslint",
  "root": true,
  "ecmaFeatures": {
    "jsx": true,
    "arrowFunctions": true,
    "modules": true,
    "templateStrings": true,
    "defaultParams": true,
    "blockBindings": true,
    "destructuring": true,
    "forOf": true,
    "restParams": true,
    "regexUFlag": true,
    "spread": true
  },
  "rules": {
    "quotes": [1, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "strict": 0,
    "semi": 2,
    "eqeqeq": 2,
    "callback-return": 1,
    "no-sync": 2,
    "handle-callback-err": 1,
    "max-depth": [1, 6],
    "max-len": [1, 100],
    "max-nested-callbacks": [1, 4],
    "no-path-concat": 2,
    "no-undef": 2,
    "no-undefined": 2,
    "no-shadow": 1,
    "no-delete-var": 2,
    "no-label-var": 2,
    "no-use-before-define": 2,
    "func-names": 1,
    "indent": [1, 4],
    "no-array-constructor": 2,
    "no-lonely-if": 2,
    "no-continue": 2,
    "no-new-object": 2,
    "no-new-wrappers": 2,
    "no-eval": 2,
    "no-fallthrough": 2,
    "no-implied-eval": 1,
    "no-loop-func": 1,
    "no-native-reassign": 2,
    "default-case": 2,
    "guard-for-in": 2,
    "no-iterator": 2,
    "no-lone-blocks": 2,
    "no-labels": 2,
    "no-new-func": 2,
    "no-proto": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-with": 2,
    "no-void": 2,
    "yoda": 2,
    "wrap-iife": 2,
    "no-redeclare": 2,
    "no-useless-concat": 1,
    "no-param-reassign": 1,
    "no-floating-decimal": 2,
    "no-caller": 1,
    "complexity": [1, 4],
    "comma-style": [2, "last"],
    "operator-linebreak": [2, "after", { "overrides": { "?": "before", ":": "before" } }]
  },
  "env": {
      "browser": true,
      "node": true
  },
  "plugins": [
    "react",
    "babel"
  ]
}